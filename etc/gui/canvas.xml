<?xml version="1.0" encoding="UTF-8" ?>

<canvas>
    <scheme name="default" inuse="yes">
        <colors>
            <background type="name" value="black"/>
            <foreground type="name" value="white"/>
            <selection  type="name" value="white"/>
        </colors>    
        <point shape="square" size="0.5"/>
        <cursor normal="normal" selection="cross" color="white"/>
    </scheme>
    
    <scheme name="UserColorScheme 1" inuse="no">
        <colors>
            <background type="name" value="black"/>
            <foreground type="name" value="white"/>
            <selection  type="name" value="white"/>
        </colors>
        <point shape="square" size="0.5"/>
        <cursor normal="normal" selection="cross" color="white"/>      
    </scheme>
</canvas>
