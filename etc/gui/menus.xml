<MenuBar>
    <Menu name="Project">
	    <MenuItem name="New Project"         accelerator="Ctrl + N" icon="document-new.png"/> 
        <MenuItem name="Open Project ..."    accelerator="Ctrl + O" icon="document-open.png"/>
        <MenuItem name="Save Project ..."    accelerator="Ctrl + S" icon="document-save.png"/>
        <MenuItem name="Save As Project ..." accelerator="Ctrl + Shift + S" icon="document-save-as.png"/>
        <MenuItem name="Separator"/>
        <MenuItem name="Close Project"       accelerator="Ctrl + X" icon="document-close.png"/>
    </Menu>
    <Menu name="Geometry">
        <MenuSubDir name="Coordinate System" accelerator="" icon="csys.png">
            <MenuItem name="New Coordinate System"    accelerator="" icon="csys-new.png"/>
            <MenuItem name="Edit Coordinate System"   accelerator="" icon="csys-edit.png"/>    
            <MenuItem name="Delete Coordinate System" accelerator="" icon="csys-delete.png"/> 
            <MenuItem name="Separator"/>
            <MenuItem name="List Coordinate Systems"  accelerator="" icon="csys-list.png"/> 
        </MenuSubDir>
        <MenuSubDir name="Point" accelerator="" icon="point.png">
             <MenuItem name="New Point"    accelerator="" icon="point-new.png"/>
             <MenuItem name="Edit Point"   accelerator="" icon="point-edit.png"/>
             <MenuItem name="Delete Point" accelerator="" icon="point-delete.png"/>
             <MenuItem name="Separator"/>
             <MenuItem name="List Points"  accelerator="" icon="point-list.png"/>
        </MenuSubDir>   
        <MenuSubDir name="Line" accelerator="" icon="generic.png">          
             <MenuItem name="New Segment" accelerator="" icon="generic.png"/>
             <MenuItem name="New Arc"     accelerator="" icon="generic.png"/>
             <MenuItem name="Edit Line"   accelerator="" icon="generic.png"/>
             <MenuItem name="Delete Line" accelerator="" icon="generic.png"/>
             <MenuItem name="Separator"/>
             <MenuItem name="Lists Lines" callback="geometry.line.lists" accelerator="" icon="invisible.png"/>
        </MenuSubDir>       
        <MenuSubDir name="Surface" accelerator="" icon="generic.png">
             <MenuItem name="Detect Surfaces" accelerator="" icon="generic.png"/>
             <MenuItem name="Delete Surfaces" accelerator="" icon="generic.png"/>
             <MenuItem name="Separator"/>
             <MenuItem name="List Surfaces" accelerator="" icon="invisible.png"/>
        </MenuSubDir>
        <MenuItem name="Separator"/>
        <MenuItem name="List Hierarchy" accelerator="" icon="hierarchy.png"/>
    </Menu>
